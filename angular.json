{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "RobosoftPolicyApp": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/robosoft-policy-app",
      "sourceRoot": "projects/robosoft-policy-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/RobosoftPolicyApp",
            "index": "projects/robosoft-policy-app/src/index.html",
            "main": "projects/robosoft-policy-app/src/main.ts",
            "polyfills": "projects/robosoft-policy-app/src/polyfills.ts",
            "tsConfig": "projects/robosoft-policy-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/robosoft-policy-app/src/favicon.ico",
              "projects/robosoft-policy-app/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/robosoft-policy-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "projects/robosoft-policy-app/src/environments/environment.ts",
                "with": "projects/robosoft-policy-app/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "RobosoftPolicyApp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "RobosoftPolicyApp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "RobosoftPolicyApp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/robosoft-policy-app/src/test.ts",
            "polyfills": "projects/robosoft-policy-app/src/polyfills.ts",
            "tsConfig": "projects/robosoft-policy-app/tsconfig.spec.json",
            "karmaConfig": "projects/robosoft-policy-app/karma.conf.js",
            "assets": [
              "projects/robosoft-policy-app/src/favicon.ico",
              "projects/robosoft-policy-app/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/robosoft-policy-app/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/robosoft-policy-app/tsconfig.app.json",
              "projects/robosoft-policy-app/tsconfig.spec.json",
              "projects/robosoft-policy-app/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/robosoft-policy-app/e2e/protractor.conf.js",
            "devServerTarget": "RobosoftPolicyApp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "RobosoftPolicyApp:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "RobosoftPolicyApp"
}
